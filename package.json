    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.10.0",
    "homebridge": "^1.5.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "typescript": "^4.2.2"
